{
  "name": "BoilerplateGeth",
  "version": "0.0.1",
  "description": "Personal boilerplate",
  "main": "index.js",
  "repository": "https://github.com/vansosnin/boilerplate-geth",
  "author": "Ivan Sosnin <vansosnin@gmail.com>",
  "scripts": {
    "start:server": "webpack --config webpack.config.server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start": "rm -rf ./dist && yarn start:server & yarn start:client",

    "comb": "gulp sass:comb",
    "stylefmt": "stylefmt --recursive ./ui",

    "lint-staged": "lint-staged",
    "stylelint-staged": "stylelint --color --syntax scss",
    "eslint-staged": "eslint --cache",

    "test:ci": "yarn eslint && yarn stylelint && yarn test -- --reporter min",
    "test": "mocha ./**/__tests__/**/*.test.js --opts ./mocha.opts",
    "stylelint": "stylelint ./**/*.scss --color --syntax scss",
    "eslint": "eslint ./**/**.js* --cache",

    "precommit": "lint-staged",
    "prepush": "yarn eslint && yarn stylelint && yarn test",
    "preversion": "yarn eslint && yarn stylelint && yarn test"
  },
