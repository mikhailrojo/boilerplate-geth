    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "jsdom": "9.12.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "node-sass": "4.5.2",
